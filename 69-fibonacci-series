public List<Integer> fibonacciSeries(Integer n) {
    //code here
    if(n==1)
        {
            return new List<Integer> {1};
        } 
    List<Integer> fib = new List<Integer>{1,1};
    Integer value = 1;
    for(Integer i=2;i<n;i++)
        {
            value += fib[i-2];
            fib.add(value);
        }
}