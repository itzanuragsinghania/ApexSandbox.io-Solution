public Boolean sameParent(Account acc, Contact con, Opportunity opp) {
    //code here
    if(acc.AccountId == null || con.AccountId== null || opp.AccountId==null)
    {
        return false;
    }
    if(acc.AccountId == con.AccountId && acc.AccountId == opp.AccountId)
    {
        return true;
    }
    else
    {
        return false;
    }

}