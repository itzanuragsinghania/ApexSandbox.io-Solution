public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fullNames = new list<String>();
    String a;
    for(Integer i=0;i<firstNames.size();i++)
    {
        a = firstNames[i] + ' ' + lastNames[i];
        fullNames.add(a);
    }
    return fullNames;
}

