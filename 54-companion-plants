public Boolean companionPlants(String plant1, String plant2) {
    //code here
    Map<String,String> nMap = new Map<String,String>
    {
        'lettuce' => 'cucumbers',
        'onions' => 'lettuce',
        'carrots' => 'onions',
        'tomatoes' => 'onions'
    };

    if((nMap.get(plant1==plant2)) ||
        (nMap.get(plant2==plant1)))
        {
            return true;
        }
    else
    {
        return false;
    }    
