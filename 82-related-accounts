public Set<Id> accountIds(List<Opportunity> opps) {
    //code here
    Set<Id> set1 = new Set<Id>();
    opps  = [Select Name, Id, AccountId from Opportunity];
    for(Opportunity opp: opps)
    {
        if(opp.Id == opp.AccountId)
        {
            set1.add(AccountId);
        }
    }
    return set1;
}