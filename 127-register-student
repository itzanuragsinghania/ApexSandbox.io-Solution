public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c acc = new apxio__Student__c();
    acc.Name = name;
    acc.apxio__Phone__c = phone;
    acc.apxio__Email__c = email;
    Insert acc;

    try{
        apxio__Student__c abc = [Select Id, apxio__Registration_Number__c, apxio__Email__c from apxio__Student__c limit 1 ];
        return abc.Id;
    }
    catch(exception e)
    {
        return null;
    }
}