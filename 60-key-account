public Boolean isKeyAccount(Account a) {
    //code here
    if(a.Industry == 'Banking' && a.AnnualRevenue <= 600,000)|| (a.Industry == 'Technology' && a.AnnualRevenue <= 800,000 )|| (a.Industry == 'Retail' && a.AnnualRevenue <= 2,000,000 )||(a.Industry == 'All others' && a.AnnualRevenue <= 500,000){
        return True;
    else{
        return False;
    }
}